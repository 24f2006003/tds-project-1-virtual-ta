{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["data/discourse_posts.json"],
        "excludeFiles": [
          "data/post_index.pkl",
          "data/post_embeddings.pkl"
        ]
      }
    }
  ],  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ]
}
